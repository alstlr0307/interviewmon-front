# API base (Railway)
REACT_APP_API_BASE=https://your-railway-url.up.railway.app
REACT_APP_API_PREFIX=/api

# Full API URL
REACT_APP_API_FULL=https://your-railway-url.up.railway.app/api

# WebSocket (AI 실시간 전송 용)
REACT_APP_WS_URL=wss://your-railway-url.up.railway.app

# CORS 설정 (Cookie 포함 요청 허용)
REACT_APP_WITH_CREDENTIALS=true

# 사용 환경 (선택)
REACT_APP_ENV=production
